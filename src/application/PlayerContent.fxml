   </rowConstraints>
   <children>
      <Label fx:id="name" alignment="TOP_CENTER" prefHeight="100.0" prefWidth="173.0" text="Name" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowSpan="2" GridPane.valignment="CENTER">
