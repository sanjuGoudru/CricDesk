<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="mainPane" maxHeight="700.0" maxWidth="700.0" minHeight="300.0" minWidth="300.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
	<left>
		<VBox fx:id="leftVBox" maxWidth="100.0" minWidth="110.0" prefWidth="120.0" BorderPane.alignment="CENTER">
			<children>
				<Button mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0">
					<VBox.margin>
						<Insets bottom="20.0" left="20.0" right="20.0" top="50.0" />
					</VBox.margin>
					<graphic>
						<ImageView fx:id="catalog" fitHeight="60.0" fitWidth="60.0">
							<image>
								<Image url="@../img/logo/catalog.png" />
							</image>
						</ImageView>
					</graphic>
				</Button>
				<Button mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0">
					<VBox.margin>
						<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
					</VBox.margin>
					<graphic>
						<ImageView fx:id="league" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="@../img/logo/league.png" />
							</image>
						</ImageView>
					</graphic>
				</Button>
				<Button mnemonicParsing="false">
					<VBox.margin>
						<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
					</VBox.margin>
					<graphic>
						<ImageView fx:id="about" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="@../img/logo/about.png" />
							</image>
						</ImageView>
					</graphic>
				</Button>
			</children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets right="10.0" />
         </padding>
		</VBox>
	</left>
	<center>
		<AnchorPane fx:id="rightAnchor" maxHeight="500.0" maxWidth="500.0" minHeight="100.0" minWidth="100.0" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
			<children>
				<ImageView fx:id="main_logo" fitHeight="200.0" fitWidth="300.0" layoutX="97.0" layoutY="45.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../img/logo/main_logo.jpg" />
					</image>
				</ImageView>
				<ImageView fx:id="main_logo2" fitHeight="41.0" fitWidth="100.0" layoutX="414.0" layoutY="304.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../img/logo/main_logo2.jpg" />
					</image>
				</ImageView>
			</children>
		</AnchorPane>
	</center>
